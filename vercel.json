{
  "rewrites": [
    {
      "source": "/candidate-assessment",
      "destination": "/candidate-assessment"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/candidate-assessment",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}